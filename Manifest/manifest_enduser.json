{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0.0",
  "id": "aba30cfc-05df-4002-b956-a70996b12a54",
  "packageName": "com.microsoft.teams.faqplus",
   "developer": {
    "name": "<<companyName>>",
    "websiteUrl": "<<websiteUrl>>",
    "privacyUrl": "<<privacyUrl>>",
    "termsOfUseUrl": "<<termsOfUseUrl>>"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "常見問題解答加 [V2]"
  },
  "description": {
    "short": "一個貼心的 Q&A  小幫手可以回答您的問題並且可以協助聯繫工程師",
    "full": "一個貼心的 Q&A 小幫手可以回答常見問題。如果無法回答的部分，將會安排工程師立即來跟您聯繫"
  },
  "accentColor": "#64A2CC",
  "bots": [
    {
      "botId": "<<botId>>",
      "scopes": [
        "personal",
        "team"
      ],
      "commandLists": [
        {
          "scopes": [
            "personal"
          ],
          "commands": [
            {
              "title": "功能導覽",
              "description": "主要功能導覽"
            },
            {
              "title": "詢問工程師",
              "description": "聯絡工程師"
            },
            {
              "title": "分享建議",
              "description": "發送建議"
            }
          ]
        }
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "staticTabs": [
    {
      "entityId": "help",
      "name": "Help",
      "contentUrl": "https://<<appDomain>>/help?theme={theme}",
      "scopes": [
        "personal"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<<appDomain>>"   
  ]
}
